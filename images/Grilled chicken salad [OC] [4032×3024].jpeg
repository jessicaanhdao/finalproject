        <script type="text/javascript">
        //detect before initial paint
        (function() {
            var isIOS = /iPhone|iPad|iPod/.test(navigator.userAgent);
            var isAndroid = /Android/.test(navigator.userAgent);
            var isWinPho = /Windows Phone/.test(navigator.userAgent);
            var htmlClass = '';

            if (isIOS || isAndroid || isWinPho) {
                htmlClass = (isWinPho) ? ' is-winpho' : (isIOS) ? ' is-ios' : ' is-android';
                htmlClass += ' is-mobile';
            }
            document.documentElement.className += htmlClass + ' has-js';
        })();
    </script>
